		oplus,support_epp_plus;
		oplus,wls_chg_fw = "IDT_P9415_45W";

		oplus,ext_pwr_en-gpio = <&pm8350c_gpios 1 0x00>;
		oplus,wrx_en-gpio = <&pm8350c_gpios 6 0x00>;
		pinctrl-names = "wrx_en_active", "wrx_en_sleep",
				"ext_pwr_en_active", "ext_pwr_en_sleep";
		pinctrl-0 = <&wrx_en_active>;
		pinctrl-1 = <&wrx_en_sleep>;
		pinctrl-2 = <&ext_pwr_en_active>;
		pinctrl-3 = <&ext_pwr_en_sleep>;

		/*
		 * | temp_min | temp_max | target_curr_ma | vol_max_mv |need wait | soc_max |
		 *
		 * temp_min: Switch to the previous level when the temperature is lower
		 *           than this value. If it is 0, do not switch.
		 * temp_max: Switch to the next level when the temperature is greater
		 *           than this value
		 * target_curr_ma: Target current in this temperature range.
		 * vol_max_mv: Maximum battery voltage.
		 * need wait: 1:Need wait for a while after switching to the next level.
		 *            0:No need to wait after switching to the next level.
		 * soc_max: Maximum soc when first entering the gear.
		 */
		oplus,fastchg-fcc_step = <  0 360 2500 4220 1 30
					  340 400 2000 4445 1 70
					  340 405 1500 4490 1 90
					  340 420 1250 4490 1 90
					  360 440 1000 4490 1 90
					  420 440  750 4490 1 90
					  420 450  600 4490 0 90>;
		oplus,max-voltage-mv = <4550>;
		oplus,fastchg_curr_max_ma = <2500>;
		oplus,wls-fast-chg-call-on-curr-ma = <600>;
		oplus,wls-fast-chg-camera-on-curr-ma = <600>;
		oplus,wls_power_mw = <50000>;
		/*
		 * | temp_min | temp_max | target_curr_ma |
		 *
		 * temp_min: Switch to the previous level when the temperature is lower
		 *           than this value. If it is 0, do not switch.
		 * temp_max: Switch to the next level when the temperature is greater
		 *           than this value
		 */
		oplus,epp_plus-skin-step = <0   360 15000
					    350 380 10000
					    360 390 6000
					    370 410 5000>;
		oplus,epp-skin-step = <0   390 10000
				       360 400 4500>;
		oplus,bpp-skin-step = <0   390 5000
				       370 400 2500>;
		oplus,epp_plus-led-on-skin-step = <0   390 8000
						   380 500 4000>;
		oplus,epp-led-on-skin-step = <0   390 8000
					      380 500 4000>;
		oplus,wls-fast-chg-led-off-strategy-data = <0   410 2500 1 0
							    390 450 1200 1 0>;
		oplus,wls-fast-chg-led-on-strategy-data = <0   340 2500 1 0
							   330 360 1000 2 0
							   350 380  800 3 1
							   370 400  600 3 2>;

		oplus,iclmax-ma = <0 0 5000  5000  5000  5000  5000 0    /*wls-bpp*/
				   0 0 10000 10000 10000 10000 5000  0    /*wls-epp*/
				   0 0 15000 15000 15000 15000 5000  0    /*wls-epp-plus*/
				   0 0 15000 15000 15000 15000 5400  0>;  /*wls-fast*/
		oplus,iclmax-batt-high-ma = <0 0 5000  5000  5000  5000  5000 0    /*wls-bpp*/
					     0 0 10000 10000 10000 10000 5000  0    /*wls-epp*/
					     0 0 10000 15000 15000 15000 5000  0    /*wls-epp-plus*/
					     0 0 10200 15000 15000 15000 5400  0>;  /*wls-fast*/

		/* fod parameter*/
		/*
		oplus,fastchg-fod-enable;
		oplus,fastchg-match-q = /bits/ 8 <0x44>;
		oplus,fastchg-fod-parm = /bits/ 8 <0xac 0x32 0xac 0x28 0xa0 0x1e
						   0x9a 0x37 0x9a 0x32 0x9f 0xc4>;
		*/

		oplus,bpp-vol-mv = <5000>;
		oplus,epp-vol-mv = <12000>;
		oplus,epp_plus-vol-mv = <12000>;
		oplus,vooc-vol-mv = <12000>;
		oplus,svooc-vol-mv = <12000>;
	};
};

&tlmm {
	oplus_wls_gpio: oplus_wls_gpio {
		cp_en_active: cp_en_active {
			mux {
				pins = "gpio70";
				function = "gpio";
			};

			config {
				pins = "gpio70";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};
		cp_en_sleep: cp_en_sleep {
			mux {
				pins = "gpio70";
				function = "gpio";
			};

			config {
				pins = "gpio70";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		cp_int_default: cp_int_default {
			mux {
				pins = "gpio44";
				function = "gpio";
			};

			config {
				pins = "gpio44";
				drive-strength = <2>;
				bias-pull-down;
				input-enable;
			};
		};

		rx_event_default: rx_event_default {
			mux {
				pins = "gpio8";
				function = "gpio";
			};

			config {
				pins = "gpio8";
				drive-strength = <2>;
				input-enable;
				bias-disable;
			};
		};

		rx_mode_default: rx_mode_default {
			mux {
				pins = "gpio65";
				function = "gpio";
			};

			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		rx_mode_active: rx_mode_active {
			mux {
				pins = "gpio65";
				function = "gpio";
			};

			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		rx_mode_sleep: rx_mode_sleep {
			mux {
				pins = "gpio65";
				function = "gpio";
			};

			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};

		rx_con_default: rx_con_default {
			mux {
				pins = "gpio92";
				function = "gpio";
			};

			config {
				pins = "gpio92";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};
	};
};

&pm8350c_gpios {
	ext_pwr_en_active: ext_pwr_en_active {
		pins = "gpio1";
		function = "normal";
		bias-disable;
		power-source = <0>;
		output-high;
		qcom,drive-strength = <3>;
	};
	ext_pwr_en_sleep: ext_pwr_en_sleep {
		pins = "gpio1";
		function = "normal";
		bias-disable;
		power-source = <0>;
		output-low;
		qcom,drive-strength = <3>;
	};

	wrx_en_active: wrx_en_active {
		pins = "gpio6";
		function = "normal";
		bias-disable;
		power-source = <1>;
		output-high;
		qcom,drive-strength = <3>;
	};
	wrx_en_sleep: wrx_en_sleep {
		pins = "gpio6";
		function = "normal";
		bias-disable;
		power-source = <1>;
		output-low;
		qcom,drive-strength = <3>;
	};
};

&pm8350c_gpios {
	rx_en_sleep: rx_en_sleep {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		power-source = <0>;
		output-high;
		qcom,drive-strength = <3>;
	};
	rx_en_active: rx_en_active {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		power-source = <0>;
		output-low;
		qcom,drive-strength = <3>;
	};
};

&qupv3_se1_i2c {
	status = "ok";
	qcom,clk-freq-out = <400000>;

	oplus_hl7227: oplus,hl7227@51 {
		status = "ok";
		compatible = "oplus,hl7227-cp";
		reg = <0x51>;
		oplus,ic_type = <OPLUS_CHG_IC_CP_DIV2>;
		oplus,ic_index = <0>;

		oplus,cp_en-gpio = <&tlmm 70 0x00>;
		oplus,cp_int-gpio = <&tlmm 44 0x00>;
		pinctrl-names = "cp_en_active", "cp_en_sleep", "cp_int_default";
		pinctrl-0 = <&cp_en_active>;
		pinctrl-1 = <&cp_en_sleep>;
		pinctrl-2 = <&cp_int_default>;
	};

	oplus_p9415: oplus,p9415@3b {
		status = "ok";
		compatible = "oplus,p9415-rx";
		reg = <0x3b>;
		#address-cells = <1>;
		#size-cells = <0>;
		oplus,ic_type = <OPLUS_CHG_IC_RX>;
		oplus,ic_index = <0>;

		oplus,rx_event-gpio = <&tlmm 8 0x00>;
		oplus,rx_con-gpio = <&tlmm 92 0x00>;
		oplus,rx_en-gpio = <&pm8350c_gpios 8 0x00>;
		oplus,rx_mode-gpio = <&tlmm 65 0x00>;
		pinctrl-names = "rx_event_default", "rx_con_default",
				"rx_en_sleep", "rx_en_active",
				"rx_mode_default", "rx_mode_active",
				"rx_mode_sleep";
		pinctrl-0 = <&rx_event_default>;
		pinctrl-1 = <&rx_con_default>;
		pinctrl-2 = <&rx_en_sleep>;
		pinctrl-3 = <&rx_en_active>;
		pinctrl-4 = <&rx_mode_default>;
		pinctrl-5 = <&rx_mode_active>;
		pinctrl-6 = <&rx_mode_sleep>;
	};
};
