&tlmm {
	cam_sensor_double_front_vana_active: cam_sensor_double_front_vana_active {
		/* RESET */
		mux {
			pins = "gpio93";
			function = "gpio";
		};

		config {
			pins = "gpio93";
			output-high;
			bias-pull-up;
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_double_front_vana_suspend: cam_sensor_double_front_vana_suspend {
		/* RESET */
		mux {
			pins = "gpio93";
			function = "gpio";
		};

		config {
			pins = "gpio93";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

